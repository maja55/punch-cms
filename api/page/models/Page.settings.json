{
  "connection": "default",
  "collectionName": "aboutpages",
  "info": {
    "name": "page",
    "description": "content shown at /about page"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "TopTextBanner": {
      "group": "text_banner",
      "type": "group"
    },
    "heroimage": {
      "collection": "file",
      "via": "related",
      "plugin": "upload"
    },
    "Video": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "RoloBanner": {
      "group": "link",
      "type": "group"
    },
    "hideContactForm": {
      "type": "boolean"
    },
    "TextBanner": {
      "group": "text_banner",
      "type": "group"
    },
    "ExitTextBanner": {
      "group": "text_banner",
      "type": "group"
    },
    "projects": {
      "collection": "project"
    },
    "news": {
      "collection": "news"
    },
    "page": {
      "unique": true,
      "enum": [
        "home",
        "about",
        "approach",
        "contact"
      ],
      "required": true,
      "type": "enumeration"
    },
    "steps": {
      "collection": "step"
    }
  }
}