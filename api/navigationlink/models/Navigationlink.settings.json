{
  "connection": "default",
  "collectionName": "navigationlinks",
  "info": {
    "name": "navigationlink",
    "description": "Pages that will be listed in site navigation menu"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "label": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "href": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "symbol": {
      "type": "string",
      "maxLength": 1,
      "required": true,
      "unique": true
    }
  }
}