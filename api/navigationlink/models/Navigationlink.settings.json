{
  "connection": "default",
  "collectionName": "navigationlinks",
  "info": {
    "name": "navigationlink",
    "description": "Pages that will be listed in site navigation menu"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "label": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "pathname": {
      "enum": [
        "about",
        "approach",
        "contact",
        "home",
        "news",
        "services",
        "work"
      ],
      "type": "enumeration"
    }
  }
}